<?xml version="1.0"?>
<launch>

    <arg name="drone_namespace"        default="dronexx"/>

    <group ns="$(arg drone_namespace)">
        <node name="cam_odom_correction_tf" pkg="tf" type="static_transform_publisher" args="-0.114 0.005 0.035 0 -0.314159 0 $(arg drone_namespace)/cam_odom_corr $(arg drone_namespace)/cam_odom_odom_frame 10"/>
        <node name="mavros_temp_tf"         pkg="tf" type="static_transform_publisher" args="0 0 0 -1.57 0 0 $(arg drone_namespace)/cam_odom_odom_frame $(arg drone_namespace)/cam_odom_mavros_temp_frame 10"/>
        <node name="mavros_viz_tf"          pkg="tf" type="static_transform_publisher" args="0 0 0 1.57 0 0 $(arg drone_namespace)/cam_odom_mavros_temp_frame $(arg drone_namespace)/cam_odom_mavros_frame 10"/>
        <!-- <node name="" pkg="tf" type="static_transform_publisher" args=""/> -->

        <node name="logger" pkg="swarm_controllers" type="loggerAll.py"/> 
        <node name="gui_service_servers" pkg="swarm_mapping" type="service_server.py"/>
        <node name="hostMasterDiscovery" pkg="master_discovery_fkie" type="master_discovery"/>
        <node name="hostMasterSync" pkg="master_sync_fkie" type="master_sync"/>
        <node name="led_controller" pkg="swarm_controllers" type="led_controller"/>
        <node name="" pkg="" type=""/>
    </group>

</launch>